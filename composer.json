{
    "name": "neos/ldap",
    "type": "neos-package",
    "description": "Ldap Authentication for Flow",
    "license": "MIT",
    "require": {
        "php": "^7.0",

        "neos/eel": "^4.0 | ^5.0",
        "neos/flow": "^4.0 | ^5.0",
        "neos/neos": "^3.0 | ^4.0",
        "neos/utility-arrays": "^4.0 | ^5.0",
        "neos/utility-files": "^4.0 | ^5.0",
        "symfony/ldap": "^3.1 | ^4.0",
        "symfony/yaml": "<5.0"
    },
    "replace": {
        "typo3/ldap": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Neos\\Ldap\\": "Classes"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Neos.Ldap"
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
